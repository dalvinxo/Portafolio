{
  "name": "portafolio",
  "jest": {
    "testEnviroment": true,
    "verbose": true
  },
  "version": "1.0.0",
  "description": "this repository for description sheet life.",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "test": "jest",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "dev": "concurrently -n \"watch:css,watch:server\" -c \"blue,yellow\" \"npm run watch:scss\" \"npm run lite\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalvinxo/Portafolio.git"
  },
  "author": "Dalvin D. Molina O.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dalvinxo/Portafolio/issues"
  },
  "homepage": "https://github.com/dalvinxo/Portafolio#readme",
  "devDependencies": {
    "jest": "^27.2.0",
    "lite-server": "^2.6.1",
    "onchange": "^7.1.0",
    "node-sass": "^6.0.1",
    "concurrently": "^6.2.1"
  },
  "dependencies": {
    "axios": ">=0.21.2"
  }
}
